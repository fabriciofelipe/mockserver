{
    "priority": 3,
    "request": {
        "method": "GET",
        "urlPattern": "/api/balancemanagement/v2/balance/([0-9]{11})",
        "headers": {
            "Content-Type": {
                "equalTo": "application/json"
            },
            "Authorization": {
                "matches": "Basic (.+)"
            }
        }
    },
    "response": {
        "status": 200,
        "headers": {
            "Content-Type": "application/json",
            "Access-Control-Allow-Origin": "*",
            "Access-Control-Allow-Headers":"Authorization, Origin, X-Requested-With, Content-Type, Accept, x-cpf, x-origin-application, x-origin-channel"
        },
        "bodyFileName": "ApiGateway/BalanceManagement/sucesso.json"
    }
}