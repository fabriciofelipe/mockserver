{
    "priority": 1,
    "request": {
        "method": "POST",
        "url": "/recaptcha/api/siteverify",
        "bodyPatterns": [
            {
                "matches": ".*response=abcdefgh-123456789.*",
                "ignoreArrayOrder": true               
            }
        ]
    },
    "response": {
        "status": 200,
        "headers":
        {
            "Content-Type": "application/json"
        },
        "body": "{ \"success\": true }"
    }
}